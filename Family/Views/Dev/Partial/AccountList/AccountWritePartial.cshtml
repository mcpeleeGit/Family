@model JangBoGo.Model.Param.Common.CommonParam

<form name="frmWrite" id="frmWrite" action="/Dev/AccountWrite" method="post">
    <table class="table table-striped">
        <tr>
            <th>내용</th>
            <td><input type="text" name="Contents" size="10" value="" /></td>
        </tr>
        <tr>
            <th>금액</th>
            <td><input type="text" name="Price" size="10" value="" /></td>
        </tr>
        <tr>
            <th>처리일</th>
            <td><input type="text" name="ProcDate" size="10" value="" /></td>
        </tr>
        <tr>
            <th>입출금구분</th>
            <td>@Html.DropDownList("InOutCode", Model.GetSelectList("inOutCode"))</td>
        </tr>
        <tr>
            <th>유형</th>
            <td>@Html.DropDownList("CaseCode", Model.GetSelectList("caseCode"))</td>
        </tr>
        <tr>
            <th>카드은행구분</th>
            <td>@Html.DropDownList("CardBankCode", Model.GetSelectList("cardBankCode"))</td>
        </tr>
    </table>
    <input type="button" value="등록" id="confirmWrite" class="btn btn-primary btn-lg" />
</form>